<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:controls="clr-namespace:Sudoku.Views.Controls"
             mc:Ignorable="d"
             x:Class="Sudoku.Views.Controls.BigCell">
    <Border BorderBrush="Black" BorderThickness="1">
        <Grid>
            <Grid ColumnDefinitions="*, *, *" RowDefinitions="*, *, *">
                <controls:NumberCell  Name="NumberCell00" Grid.Column="0" Grid.Row="0" />
                <controls:NumberCell Name="NumberCell01" Grid.Column="0" Grid.Row="1" />
                <controls:NumberCell Name="NumberCell02" Grid.Column="0" Grid.Row="2" />
                <controls:NumberCell Name="NumberCell10" Grid.Column="1" Grid.Row="0" />
                <controls:NumberCell Name="NumberCell11" Grid.Column="1" Grid.Row="1" />
                <controls:NumberCell Name="NumberCell12" Grid.Column="1" Grid.Row="2" />
                <controls:NumberCell Name="NumberCell20" Grid.Column="2" Grid.Row="0" />
                <controls:NumberCell Name="NumberCell21" Grid.Column="2" Grid.Row="1" />
                <controls:NumberCell Name="NumberCell22" Grid.Column="2" Grid.Row="2" />
            </Grid>
        </Grid>
    </Border>
</UserControl>